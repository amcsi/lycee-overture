{
  "name": "amcsi/lycee-overture",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-pdo_mysql": "*",
    "ext-simplexml": "*",
    "astrotomic/laravel-translatable": "^11.6",
    "cakephp/chronos": "^2.3",
    "cloudinary/cloudinary_php": "^1.10",
    "doctrine/dbal": "^2.8",
    "erusev/parsedown": "^1.7",
    "guzzlehttp/guzzle": "^7.0.1",
    "guzzlehttp/psr7": "^1.5",
    "laravel/framework": "9.*",
    "laravel/sanctum": "^2.4",
    "laravel/tinker": "^2.0",
    "laravel/ui": "^3.0",
    "league/csv": "^9.1",
    "league/flysystem-memory": "3.*",
    "league/fractal": "^0.17.0",
    "mbilbille/jpnforphp": "^0.7.0",
    "nikic/iter": "^2.0",
    "php-http/curl-client": "^2.2",
    "rollbar/rollbar-laravel": "^7.0",
    "snoeren-development/laravel-discord-webhook-channel": "^1.3",
    "swooletw/laravel-swoole": "^2.12",
    "symfony/stopwatch": "^4.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "barryvdh/laravel-ide-helper": "^2.4",
    "eaglewu/swoole-ide-helper": "dev-master",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^6.1",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "classmap": [
      "database/seeders",
      "database/factories"
    ],
    "psr-4": {
      "amcsi\\LyceeOverture\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/",
      "Database\\Tools\\": "database/tools/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
