{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "ext-simplexml": "*",
    "astrotomic/laravel-translatable": "^11.6",
    "beyondcode/laravel-self-diagnosis": "^1.0.2",
    "cakephp/chronos": "^1.1",
    "cloudinary/cloudinary_php": "^1.10",
    "dingo/api": "^2.0.0",
    "doctrine/dbal": "^2.8",
    "erusev/parsedown": "^1.7",
    "fideloper/proxy": "^4.0",
    "guzzlehttp/guzzle": "^6.3",
    "guzzlehttp/psr7": "^1.5",
    "laravel/framework": "6.*",
    "laravel/tinker": "^1.0",
    "league/csv": "^9.1",
    "league/flysystem-memory": "^1.0",
    "league/fractal": "^0.17.0",
    "mbilbille/jpnforphp": "^0.7.0",
    "nikic/iter": "^2.0",
    "php-http/curl-client": "^1.7",
    "rollbar/rollbar-laravel": "^5.0",
    "swooletw/laravel-swoole": "^2.4",
    "symfony/stopwatch": "^4.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "barryvdh/laravel-ide-helper": "^2.4",
    "eaglewu/swoole-ide-helper": "dev-master",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^2.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "amcsi\\LyceeOverture\\": "app/",
      "database\\migrations\\tools\\": "database/migrations/tools/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
